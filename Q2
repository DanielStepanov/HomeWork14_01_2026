
index: int = 0
min_score: int = 101
max_score: int = -1
sum_score: int = 0

while index < 20:
    score: int = int(input())
    if score <= 100 and score >= 0:
        if score > max_score:
            max_score = score

        if score < min_score:
            min_score = score

        sum_score += score
        index += 1

average_score: float = sum_score / 20
print("the average score is:", average_score)
print("the highest score is:", max_score)
print("the lowest score is:", min_score)
